{
  "name": "sAIS",
  "version": "1.1.0",
  "description": "Server-AIS self-reporting service for Garmin/DeLorme inReach/Explorer devices",
  "license": "GPL-3.0-only",
  "repository": "infinitedescent/s-ais",
  "author": {
    "name": "Hays Clark",
    "email": "hays@infinitedescent.com",
    "url": "infinitedescent.com"
  },
  "bin": "bin/sais",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "node server.js",
    "dev-sais": "nodemon -r dotenv/config --watch 'lib' --watch 'bin' --exec 'node' bin/sync",
    "dev-server": "nodemon -r dotenv/config -e js,pug --watch server.js --watch 'views/**/*.pug' --watch lib --watch public --watch routes ./server.js",
    "test": "xo && nyc ava",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "keywords": [
    "marinetraffic",
    "marine traffic",
    "delorme",
    "inreach",
    "explorer",
    "garmin",
    "ais",
    "cli-app",
    "cli",
    "heroku"
  ],
  "dependencies": {
    "body-parser": "1.18.2",
    "cheerio": "0.22.0",
    "express": "4.16.0",
    "method-override": "2.3.10",
    "moment": "2.29.2",
    "nib": "1.1.2",
    "pug": "2.0.3",
    "request": "2.88.0",
    "request-promise-native": "1.0.3",
    "sendgrid": "^2.0.0",
    "stylus": "0.54.5",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "ava": "2.0.0",
    "coveralls": "3.0.3",
    "dotenv": "^8.0.0",
    "husky": "^2.3.0",
    "nodemon": "1.19.1",
    "nyc": "14.1.1",
    "proxyquire": "1.8.0",
    "sinon": "2.3.5",
    "snyk": "1.171.1",
    "xo": "0.24.0"
  },
  "xo": {
    "ignore": "public/js/*.js"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "snyk": true
}
